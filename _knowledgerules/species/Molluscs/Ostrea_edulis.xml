            <Content>
							<type>equation</type>
							<description>"Equation to calculate the Log Odds (Logit) to HSI for Ostrea edulis"</description>
							<author>"Marc Weeber"</author>
							<date>2023-01-10</date>
							<references>
								<reference author="P. Herman and F.F. van Rees" year="2021" title="Mapping Reef forming North Sea Species" identification="11207716-000-ZKS-0001" journal=""/>
							</references>
							<Equation>
								<SimpleEquation>
									<equation>"exp(Odds_O_edulis) / (1 + exp(Odds_O_edulis))"</equation>
								</SimpleEquation>
							</Equation>
							<Parameters>
								<valuesScalar layername="Odds_O_edulis" type="scalar">
									<parameter min_input="-200" max_input="200"/>
								</valuesScalar>
							</Parameters>
						</Content>
						<outputLayers>
						 <layer name="HGI_O_edulis">
								<parameter_name>HGI_O_edulis</parameter_name>
								<parameter_cat>Mollusc</parameter_cat>
								<period>Undefined</period>
								<position>Undefined</position>
								<unit>"-"</unit>
								<statistic>none</statistic>
								<layer_filename>"HGI_O_edulis"</layer_filename>
