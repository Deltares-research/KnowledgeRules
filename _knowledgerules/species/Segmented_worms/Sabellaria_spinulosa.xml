            <Content>
							<type>equation</type>
							<description>"Equation to calculate the Log Odds (logit) to HSI for Sabellaria spinulosa"</description>
							<author>"Marc Weeber"</author>
							<date>2023-01-10</date>
							<references>
								<reference author="P. Herman and F.F. van Rees" year="2021" title="Mapping Reef forming North Sea Species" identification="11207716-000-ZKS-0001" journal=""/>
							</references>
							<Equation>
								<SimpleEquation>
									<equation>"exp(Odds_S_spinulosa) / (1 + exp(Odds_S_spinulosa))"</equation>
								</SimpleEquation>
							</Equation>
							<Parameters>
								<valuesScalar layername="Odds_S_spinulosa" type="scalar">
