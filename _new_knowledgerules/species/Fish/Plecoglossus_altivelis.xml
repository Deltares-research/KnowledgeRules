								<layer_filename>FlowVelocity</layer_filename>
								<unit>"m/s"</unit>
								<statistic>maximum</statistic>
								<description>TOBEFILLED</description>
							</layer>
						</inputLayers>
						<Content>
							<type>scalar</type>
							<description>"Maximum tolerable flow velocity for the Ayu fish species"</description>
							<author>"Mijke van Oorschot"</author>
							<date>2019-06-20</date>
							<references>
								<reference author="M. Van Oorschot" year="2019" title="" journal="" identification="">
									<html/>
								</reference>
							</references>
							<valuesScalar>
								<parameter input="0" output="0"/>
