								<parameter input="0" output="0"/>
								<parameter input="0.13" output="1"/>
								<parameter input="1" output="1"/>
								<parameter input="25" output="1"/>
							</valuesScalar>
						</Content>
						<outputLayers>
							<layer name="HSI_WaterDepth">
								<parameter_cat>HSI</parameter_cat>
								<layer_filename>HSI_WaterDepth</layer_filename>
								<unit>factor</unit>
								<statistic>average</statistic>
								<description>TOBEFILLED</description>
							</layer>
